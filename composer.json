{
    "name": "tweekersnut/cache",
    "description": "A high-performance, pluggable PHP caching library with Redis, file, session, and memory drivers.",
    "type": "library",
    "keywords": ["cache", "caching", "redis", "file-cache", "session-cache", "performance", "psr"],
    "license": "MIT",
    "authors": [
        {
            "name": "TweekersNut Network",
            "email": "dev@tweekersnut.com"
        }
    ],
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "suggest": {
        "ext-redis": "Required for Redis cache driver",
        "predis/predis": "Alternative Redis client if ext-redis is not available"
    },
    "autoload": {
        "psr-4": {
            "TweekersNut\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TweekersNut\\Cache\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
